{
  "name": "@ts-livedata/core",
  "version": "0.1.2",
  "description": "A TypeScript LiveData library.",
  "scripts": {
    "test": "mocha -r ts-node/register src/test/**/*.spec.ts",
    "coverage": "nyc mocha -r ts-node/register src/test/**/*.spec.ts",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir build --extensions \".ts,.tsx\" --source-maps inline"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "private": false,
  "keywords": [],
  "author": "Brendan Goldberg",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^8.0.0",
    "chai": "^4.2.0",
    "mocha": "^8.0.1",
    "nyc": "^15.1.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7",
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3"
  }
}
