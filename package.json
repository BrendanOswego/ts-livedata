{
  "name": "@ts-livedata/core",
  "version": "0.1.0",
  "description": "A TypeScript LiveData library.",
  "scripts": {
    "test": "mocha -r ts-node/register src/test/**/*.spec.ts",
    "coverage": "nyc mocha -r ts-node/register src/test/**/*.spec.ts",
    "prebuild": "rimraf build",
    "build": "rollup -c config/rollup.config.js && npm run copypackage",
    "copypackage": "cp -rf package.json build",
    "release": "cd build && npm publish"
  },
  "private": false,
  "keywords": [
    "LiveData",
    "TypeScript",
    "Observer Pattern"
  ],
  "author": "Brendan Goldberg",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-typescript": "^5.0.2",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^8.0.0",
    "chai": "^4.2.0",
    "mocha": "^8.0.1",
    "nyc": "^15.1.0",
    "path": "^0.12.7",
    "rollup": "^2.21.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7",
    "tslib": "^2.0.0"
  }
}
